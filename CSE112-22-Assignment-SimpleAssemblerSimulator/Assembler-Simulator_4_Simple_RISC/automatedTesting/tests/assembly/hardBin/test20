var crr
var f
var ct
mov R1 $15
mov R2 $1
mov R5 $8
st R2 f
st R2 crr
st R2 ct
sub R1 R2 R1
mov FLAGS R6
rs R6 $3
cmp R6 R2
je end
loop: ld R6 ct
ld R3 f
mul R1 R3 R3
mov FLAGS R4
and R4 R5 R4
cmp R4 R5
je wrong
add R2 R6 R6
st R6 ct
st R3 f
sub R1 R2 R1
cmp R1 R2
je end
jmp loop
wrong: st R0 crr
end: hlt